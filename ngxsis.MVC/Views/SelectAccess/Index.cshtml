@model ngxsis.ViewModel.SelectAccessViewModel

@{
    Layout = null;
    if (Session["userID"] == null)
    {
        Response.Redirect("~/Login/Index"); //bila belum login sistem akan re-direct ke halaman login
    }
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div> 
        <h1>Select Access</h1>
        <h2>Hai, @Session["userName"]</h2>
        <h3>ID : @Session["userID"]</h3>
        <a href="@Url.Action("Logout","Login")">Logout</a>
    </div>
</body>
</html>
